{
  "name": "playwright_javascript_advanced",
  "version": "1.0.0",
  "description": "Automation software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "test:chromium": "playwright test --config=playwright.config.js --project=chromium",
    "test:firefox": "playwright test --config=playwright.config.js --project=firefox",
    "test:e2e:chromium": "playwright test --config=e2e.config.js --project=chromium",
    "test:e2e:firefox": "playwright test --config=e2e.config.js --project=firefox",
    "test:e2e:webkit": "playwright test --config=e2e.config.js --project=webkit",
    "test:e2e:chrome": "playwright test --config=e2e.config.js --project=GoogleChrome",
    "test:e2e:mobileChrome": "playwright test --config=e2e.config.js --project=MobileChrome",
    "test:visual:chromium": "playwright test --config=visual.config.js --project=chromium",
    "test:visual:firefox": "playwright test --config=visual.config.js --project=firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PavelBohdan/Playwright_JavaScript_Advanced.git"
  },
  "author": "PavelBohdan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PavelBohdan/Playwright_JavaScript_Advanced/issues"
  },
  "homepage": "https://github.com/PavelBohdan/Playwright_JavaScript_Advanced#readme",
  "dependencies": {
    "prettier": "^2.8.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2"
  }
}